pipeline {
  agent any
  stages {
    stage('Start') {
      steps {
        echo 'Start Message'
      }
    }

    stage('Build') {
      steps {
        echo 'Build'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy'
      }
    }

    stage('Finish') {
      steps {
        echo 'Finish'
      }
    }

  }
}